import org.jointheleague.graphical.robot.Robot;

public class DrawMe {
public static void main(String[] args) {
	
	Robot Alex= new Robot();
	
	Alex.penDown();
	Alex.turn(20);
	Alex.move(500);
	Alex.turn(135);
	Alex.move(500);
	Alex.turn(180);
	Alex.move(350);
	Alex.turn(300);
	Alex.move(150);
	Alex.penUp();
	Alex.move(200);
	
	
}
}
